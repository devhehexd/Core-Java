import java.util.Scanner;

public class DoDemo {

	public static void main(String[] args) {
		
		//4! = 4 * 3 * 2 * 1
		
		Scanner sc = new Scanner(System.in);
		System.out.println("ëª‡ Factorial? : ");
		int su = sc.nextInt();
		int answer = 1;
		
		for (int i = su; i > 0; i--) {
			answer *= i;
		}
		System.out.println(su + "! = " + answer);
				

	}

}
